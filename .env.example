# Application Configuration
VITE_APP_NAME="SandBlox"
VITE_APP_DESCRIPTION="Deploy, Secure, and Manage Your Smart Contracts with Ease"
VITE_APP_URL="http://localhost:5173"

# WalletConnect Configuration
VITE_WALLET_CONNECT_PROJECT_ID=

# Network Configuration
VITE_DEVNET_RPC_URL=
VITE_DEVNET_CHAIN_ID=
VITE_DEVNET_NAME=
VITE_DEVNET_EXPLORER_URL=

# Security & Performance
VITE_MAX_BATCH_SIZE=100
VITE_REQUEST_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000

# Feature Flags
VITE_ENABLE_TESTNET=false
VITE_ENABLE_LOCAL_NODE=false
VITE_ENABLE_REMOTE_NODE=true
VITE_ENABLE_DEBUG_LOGS=true

# Contract Deployment
VITE_DEFAULT_GAS_LIMIT=6000000
VITE_GAS_PRICE_BUFFER=1.1
VITE_CONFIRMATION_BLOCKS=2

# API Keys
VITE_INFURA_API_KEY=your_infura_key_here
VITE_ETHERSCAN_API_KEY=your_etherscan_key_here

# Library Addresses
VITE_LIBRARY_MULTI_PHASE_SECURE_OPERATION=""
