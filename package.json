{
  "name": "sand-blox",
  "version": "1.0.0",
  "private": true,
  "description": "Deploy, Secure, and Manage Your Smart Contracts with Ease - Powered by Particle Abstraction",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:remote": "vite --mode production",
    "dev:local": "vite --mode development",
    "build": "tsc && vite build",
    "prebuild": "tsx scripts/copy-to-public.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "postinstall": "git submodule update --init --recursive",
    "update:submodules": "git submodule update --remote --merge",
    "contracts:build": "cd src/Guardian && forge build",
    "contracts:test": "cd src/Guardian && forge test"
  },
  "dependencies": {
    "@ionic/react": "^8.4.3",
    "@openzeppelin/contracts": "^4.9.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@safe-global/api-kit": "^4.0.0",
    "@safe-global/protocol-kit": "^6.1.0",
    "@safe-global/safe-core-sdk-types": "^5.1.0",
    "@safe-global/types-kit": "^3.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.66.7",
    "@types/react-router-dom": "^5.3.3",
    "@wagmi/core": "^2.16.4",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/universal-provider": "^2.11.2",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^5.7.2",
    "framer-motion": "^11.0.5",
    "ionicons": "^7.4.0",
    "jotai": "^2.12.1",
    "lucide-react": "^0.475.0",
    "mermaid": "^11.10.0",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.5.2",
    "remark-gfm": "^4.0.1",
    "solc": "^0.8.25",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.23.3",
    "wagmi": "^2.14.11",
    "zod": "^3.24.2",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.11.24",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.0.7",
    "autoprefixer": "^10.4.18",
    "esbuild": "^0.25.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^11.0.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^6.3.4",
    "vitest": "^3.0.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
